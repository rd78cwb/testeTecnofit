# Force text and normalize line endings
* text=auto

# Treat these as binary
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.zip binary
*.tar binary
*.gz binary
*.ttf binary
*.woff binary
*.woff2 binary

# Blade templates
*.blade.php linguist-language=HTML

# Treat PHP files as PHP
*.php linguist-language=PHP

# Ignore migration changes in diffs
database/migrations/*.php -diff
